{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@content/*": [
        "src/content/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    },
    "strictNullChecks": true,
    "allowJs": true,
    "types": [
      "@astrojs/image/client"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}